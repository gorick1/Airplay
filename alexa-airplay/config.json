{
  "name": "Alexa Music Controller",
  "version": "2.0.0",
  "slug": "alexa-airplay",
  "description": "Control music playback on Amazon Echo devices via Home Assistant",
  "url": "https://github.com/gorick1/Airplay/tree/main/alexa-airplay",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf"
  ],
  "startup": "services",
  "boot": "auto",
  "homeassistant_api": true,
  "hassio_api": true,
  "hassio_role": "default",
  "init": false,
  "ingress": true,
  "ingress_port": 8099,
  "panel_icon": "mdi:speaker-multiple",
  "panel_title": "Alexa Music",
  "panel_admin": true,
  "options": {
    "debug_logging": false
  },
  "schema": {
    "debug_logging": "bool?"
  }
}

